<Window x:Name="Binder" x:Class="BinderWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BinderWPF"
        mc:Ignorable="d"
        Title="Binder" Height="410" Width="300" HorizontalAlignment="Center" VerticalAlignment="Center">
    <Grid Margin="0,0,2,-2">
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="320" Margin="10,10,0,0" VerticalAlignment="Top" Width="270"/>
        <Button x:Name="addButton" Content="Add bind" HorizontalAlignment="Left" Margin="10,341,0,0" VerticalAlignment="Top" Width="75" Click="addButton_Click_1"/>
        <Button x:Name="removeButton" Content="Remove" HorizontalAlignment="Left" Margin="205,341,0,0" VerticalAlignment="Top" Width="75" Click="removeButton_Click"/>
        <Button x:Name="startButton" Content="Start" HorizontalAlignment="Left" Margin="110,341,0,0" VerticalAlignment="Top" Width="75" Click="startButton_Click"/>
    </Grid>
</Window>
